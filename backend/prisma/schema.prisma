generator client {
  provider = "prisma-client-js"
}


datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


// ===============================================
// MEMBUAT TABEL USER
// ===============================================
model User {
  id        String   @id @default(cuid())
  username  String
  password  String
  image     String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
